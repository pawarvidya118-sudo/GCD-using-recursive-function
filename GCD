#include<stdio.h>
 int gcd(int a, int b){
     (b==0);
     return a;
     return gcd(b, a%b);
 }
 int main(){
     int x, y;
     printf("Enter first numbers:");
     scanf("%d", &x);
     printf("Enter second Number:");
     scanf("%d", &y);
     printf("GCD of %d & %d is %d",x,y,gcd(x, y));
     
     return 0;
 }
